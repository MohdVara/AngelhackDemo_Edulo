    <section id="Search">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Find Now</h2>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                 <%= form_tag courses_path, :method => 'get' do %>
                        <div class="row">
                            <div class="col-md-6">
                           
                                <div class="form-group">

                                    <input type="text" name="name" class="form-control" placeholder="Your Name *" id="Name" >
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="text" name="interest" class="form-control" placeholder="Your Interest *" id="interest" >
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="text" name="qualification"class="form-control" placeholder="Certificate *" id="qualification" required data-validation-required-message="Please enter your interest.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="row clearfix">


                                
    	<div class="col-md-6 table-responsive">
			<table class="table table-bordered table-sortable table-responsive" id="tab_logic">
				<thead>
					<tr >
						<th class="text-center">
							Subject
						</th>
						<th class="text-center">
							Result
						</th>
        				<th class="text-center" style="border-top: 1px solid #ffffff; border-right: 1px solid #ffffff;">
						</th>
					</tr>
				</thead>
				<tbody>
    				<tr id='addr0' data-id="0" class="hidden">
						<td data-name="name">
						    <input type="text" name='Subject'  placeholder='Subject' class="form-control"/>
						</td>
						<td data-name="mail">
						    <input type="text" name='mail0' placeholder='Result' class="form-control"/>
						</td>
                        <td data-name="del">
                            <button nam"del0" class='btn btn-danger glyphicon glyphicon-remove row-remove'></button>
                        </td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
                            <div class="clearfix"></div>
                            <div class="container">
    
	<a id="add_row" class="btn btn-default pull-right">Add Row</a>
</div>


                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">Search</button>
                            </div>
                        </div>
                    <% end %>
                </div>
            </div>
        </div>
    </section>



